{
	"servers": {
		"huggingface/hf-mcp-server": {
			"type": "http",
			"url": "https://huggingface.co/mcp?login",
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		},
		"microsoft/markitdown": {
			"type": "stdio",
			"command": "uvx",
			"args": [
				"markitdown-mcp==0.0.1a4"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		},
		"io.github.ChromeDevTools/chrome-devtools-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"chrome-devtools-mcp@0.10.2"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "0.10.2"
		},
		"io.github.upstash/context7": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@upstash/context7-mcp@1.0.30"
			],
			"env": {
				"CONTEXT7_API_KEY": "${input:CONTEXT7_API_KEY}"
			},
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.30"
		},
		"io.github.github/github-mcp-server": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp/",
			"headers": {
				"Authorization": "${input:Authorization}"
			},
			"gallery": "https://api.mcp.github.com",
			"version": "0.23.0-gosdk"
		},
		"com.postman/postman-mcp-server": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@postman/postman-mcp-server@2.5.1"
			],
			"env": {
				"POSTMAN_API_KEY": "${input:POSTMAN_API_KEY}"
			},
			"gallery": "https://api.mcp.github.com",
			"version": "2.5.1"
		},
		"microsoft/devbox-mcp-server": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@microsoft/devbox-mcp@latest",
				"@microsoft/devbox-mcp@latest"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		},
		"zapier/zapier-mcp": {
			"type": "http",
			"url": "{zapier_mcp_url}",
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		},
		"box/mcp-server-box-remote": {
			"type": "http",
			"url": "https://mcp.box.com",
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		}
	},
	"inputs": [
		{
			"id": "CONTEXT7_API_KEY",
			"type": "promptString",
			"description": "API key for authentication",
			"password": true
		},
		{
			"id": "Authorization",
			"type": "promptString",
			"description": "Authentication token (PAT or App token)",
			"password": true
		},
		{
			"id": "POSTMAN_API_KEY",
			"type": "promptString",
			"description": "A valid Postman API key.",
			"password": true
		}
	]
}